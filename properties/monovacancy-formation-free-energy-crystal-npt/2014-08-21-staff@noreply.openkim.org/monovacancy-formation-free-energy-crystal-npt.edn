  "reservoir-cohesive-free-energy" {
    "type"         "float"
    "has-unit"     true
    "extent"       []
    "required"     true
    "description" "The cohesive free energy of the reservoir crytal under the specified temperature and stress conditions."
  }
  "formation-free-energy" {
    "type"         "float"
    "has-unit"     true
    "extent"       []
    "required"     true
    "description" "The Helmholtz free energy of formation associated with extracting the 'host-removed-atom' from the host crystal at the specified temperature and stress and adding it to the reservoir crystal at a possibly different temperature and stress."
  }
